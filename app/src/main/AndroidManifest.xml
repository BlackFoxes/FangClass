<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.ucai.blackfox.fangclass">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".controller.activitys.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".controller.activitys.MainActivity" />
        <activity android:name=".controller.activitys.ShopDetailsActivity" />
        <activity android:name=".controller.activitys.LoginActivity" />
        <activity android:name=".controller.activitys.RegisterActivity" />
        <activity android:name=".controller.activitys.TransformActivity"></activity>
    </application>

</manifest>